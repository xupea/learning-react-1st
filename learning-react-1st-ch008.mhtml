                <code class=3D"nx">c</code> <code class=3D"o">=3D&gt;</code=
> <code class=3D"nx">color</code><code class=3D"p">(</code><code class=3D"n=
x">c</code><code class=3D"p">,</code> <code class=3D"nx">action</code><code=
 class=3D"p">)</code>
            <code class=3D"p">)</code>
        <code class=3D"k">case</code> <code class=3D"nx">C</code><code clas=
s=3D"p">.</code><code class=3D"nx">REMOVE_COLOR</code> <code class=3D"o">:<=
/code>
            <code class=3D"k">return</code> <code class=3D"nx">state</code>=
<code class=3D"p">.</code><code class=3D"nx">filter</code><code class=3D"p"=
>(</code>
                <code class=3D"nx">c</code> <code class=3D"o">=3D&gt;</code=
> <code class=3D"nx">c</code><code class=3D"p">.</code><code class=3D"nx">i=
d</code> <code class=3D"o">!=3D=3D</code> <code class=3D"nx">action</code><=
code class=3D"p">.</code><code class=3D"nx">id</code>
            <code class=3D"p">)</code>
